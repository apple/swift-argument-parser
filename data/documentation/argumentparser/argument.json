        {
          "code" : [
            "@main",
            "struct Greet: ParsableCommand {",
            "    @Argument var name: String",
            "    @Argument var greeting: String = \"Hello\"",
            "",
            "    mutating func run() {",
            "        print(\"\\(greeting) \\(name)!\")",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
