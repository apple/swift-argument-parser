            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "specified without a label and must appear in declaration order. ",
              "type" : "text"
            },
            {
              "code" : "@Argument",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "properties with ",
